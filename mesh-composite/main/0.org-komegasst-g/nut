FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.000335; //0;

boundaryField
{
	#include "empty-front-back"
	#include "cyclic-ami"
    
    inlet
    {
        type            calculated;
        value           uniform 0.000335;
    }

    outlet
    {
        type            calculated;
        value           uniform 0.000335;
    }

    upperWall
    {
        type            calculated;
        value           uniform 0.000335;
        //type            freestream;
    	//freestreamValue 	uniform 0;
        //value           uniform 0;
    }
    
    lowerWall
    {
        type            nutkWallFunction;
        //type            nutkRoughWallFunction;
        //type            nutLowReWallFunction;
        value           uniform 0.000335;
    }

    blades
    {
        type            nutkWallFunction;
        //type            nutkWallFunction;
        //type            nutLowReWallFunction;
        value           uniform 0.000335;
    }
}


